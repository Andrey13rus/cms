{
    "name": "skeeks/cms",
    "description": "SkeekS CMS â€” control panel and tools based on php framework Yii2",
    "keywords": ["framework", "cms", "skeeks", "sx", "skeeks cms", "app", "yii2", "extension"],
    "homepage": "https://cms.skeeks.com",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/skeeks-cms/cms",
        "issues": "https://github.com/skeeks-cms/cms/issues",
        "wiki": "https://cms.skeeks.com/docs",
        "authors": "https://skeeks.com"
    },
    "authors": [
        {
            "name": "SkeekS",
            "email": "support@skeeks.com",
            "homepage": "http://skeeks.com"
        },
        {
            "name": "Semenov Alexander",
            "email": "semenov@skeeks.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-bootstrap": "^2.0",
        "skeeks/cms-backend": "^1",
        "skeeks/cms-backend-admin": "^1",
        "skeeks/cms-admin": "^2",
        "skeeks/cms-rbac": "^2",
        "skeeks/cms-marketplace": "^1",
        "skeeks/yii2-ajax-file-upload": "^1",
        "skeeks/yii2-imagine": "^2",
        "skeeks/yii2-sx": "^1.3",
        "skeeks/yii2-ckeditor": "^1",
        "skeeks/yii2-widget-simpleajaxuploader": "^1",
        "skeeks/yii2-widget-chosen": "^1",
        "skeeks/yii2-widget-highcharts": "^1",
        "skeeks/yii2-widget-codemirror": "^1",
        "kartik-v/yii2-datecontrol": "^1",
        "kartik-v/yii2-widget-datepicker": "^1",
        "kartik-v/yii2-widget-datetimepicker": "^1",
        "kartik-v/yii2-widget-colorinput": "^1",
        "mihaildev/yii2-elfinder": "1.1.3",
        "giggsey/libphonenumber-for-php": "^8.8",
        "paulzi/yii2-auto-tree": "^1.0",
        "paulzi/yii2-adjacency-list": "^2.0",
        "paulzi/yii2-materialized-path": "^2.0",
        "skeeks/yii2-slug-behavior": "^1",
        "skeeks/yii2-ya-slug": "^1",
        "hiqdev/composer-config-plugin": "^0.2",
        "bower-asset/fancybox": "^3.1",
        "bower-asset/jquery": "2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable"
    },
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\":  "src/"
        }
    },
    "extra": {
        "config-plugin": {
            "web": [
                "src/config/common.php",
                "src/config/web.php"
            ],
            "console": [
                "src/config/common.php",
                "src/config/console.php"
            ]
        }
    }
}
